<local:BasePage
    x:Class="InformationRetrievalManager.DataInstancePage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:InformationRetrievalManager"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="DataInstancePage"
    d:Background="{StaticResource BackgroundBrushKey}"
    d:DataContext="{x:Static local:DataInstancePageDesignModel.Instance}"
    d:DesignHeight="450"
    d:DesignWidth="800"
    x:TypeArguments="local:DataInstancePageViewModel"
    mc:Ignorable="d">

    <Grid>
        <TextBlock Text="Data Instance Page" />

        <Button
            VerticalAlignment="Bottom"
            Command="{Binding GoToHomePageCommand}"
            Content="GO BACK" />
    </Grid>
</local:BasePage>
